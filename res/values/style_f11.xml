<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Floor_Name_F11">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">28sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Floor_Layout_F11">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>

    <style name="Floor_Main_Layout_F11">
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">10dp</item>
    </style>

    <style name="Floor_Line_F11">
        <item name="android:layout_height">2dp</item>
        <item name="android:background">#ffffff</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="Floor_Tag_F11">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="Floor_Shops_F11">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="Header_Layout_F11">
        <item name="android:paddingBottom">20dp</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingTop">20dp</item>
    </style>

    <style name="Header_Title_F11">
        <item name="android:padding">10dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">28sp</item>
        <item name="android:textStyle">bold</item>
    </style>

</resources>